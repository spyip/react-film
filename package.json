{
  "name": "root",
  "version": "2.1.1-0",
  "description": "React component for showing carousel just like a film strip",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "clean": "lerna run clean --parallel --stream",
    "prepublishOnly": "lerna run prepublishOnly --stream",
    "start": "concurrently --kill-others --names \"build,server\" \"lerna run start --parallel --stream\" \"npm run start:server\"",
    "start:server": "serve -p 5000",
    "test": "lerna run test"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "lerna": "^3.22.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "serve": "^11.3.2"
  }
}
