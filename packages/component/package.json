{
  "name": "react-film",
  "version": "0.0.0-0",
  "description": "React component for showing carousel just like a film strip",
  "keywords": [
    "carousel",
    "film",
    "react",
    "slideshow",
    "slide show"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "umd/**/*"
  ],
  "scripts": {
    "build": "babel --out-dir lib --ignore **/*.spec.js,**/*.test.js --source-maps inline --verbose src/",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "prestart": "npm run build",
    "start": "npm run build -- --skipInitialBuild --watch",
    "test": "echo no test specified"
  },
  "author": "Ping Yip (ping.yip@hotmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spyip/react-film.git"
  },
  "bugs": {
    "url": "https://github.com/spyip/react-film/issues"
  },
  "homepage": "https://github.com/spyip/react-film#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "glamor": "2.20.40",
    "memoize-one": "5.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
