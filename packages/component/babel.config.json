{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "transform-inline-environment-variables",
      {
        "include": ["npm_package_version"]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "modules": "commonjs"
      }
    ],
    "@babel/preset-react"
  ]
}
