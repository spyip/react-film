{
  "env": {
    "test": {
      "plugins": ["babel-plugin-istanbul"]
    }
  },
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "version": "7.20.13"
      }
    ],
    [
      "babel-plugin-transform-inline-environment-variables",
      {
        "include": ["npm_package_version"]
      }
    ]
  ],
  "presets": ["@babel/preset-react"],
  "sourceMaps": "inline"
}
