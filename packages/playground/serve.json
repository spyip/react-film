{
  "cleanUrls": false,
  "redirects": [
    {
      "destination": "/react-film/",
      "source": "/"
    }
  ],
  "rewrites": [
    {
      "destination": "/build/index.html",
      "source": "/react-film"
    },
    {
      "destination": "/build/:filename",
      "source": "/react-film/:filename"
    },
    {
      "destination": "/build/image/:filename",
      "source": "/react-film/image/:filename"
    },
    {
      "destination": "/build/static/:dirname/:filename",
      "source": "/react-film/static/:dirname/:filename"
    }
  ]
}
